{
  "name": "crypto_currencies",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://silencerweb.github.io/crypto_currencies",
  "dependencies": {
    "autoprefixer": "7.2.3",
    "axios": "^0.17.1",
    "axios-retry": "^3.0.1",
    "babel-core": "6.26.0",
    "babel-eslint": "8.1.2",
    "babel-jest": "22.0.4",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "^3.1.0",
    "babel-runtime": "6.26.0",
    "bootstrap": "4.0.0-beta.3",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "2.3.0",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "eslint": "4.14.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.40.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.0",
    "eslint-plugin-react": "7.5.1",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.6",
    "fs-extra": "5.0.0",
    "gh-pages": "^1.1.0",
    "graphql": "^0.12.3",
    "html-webpack-plugin": "2.30.1",
    "jest": "22.0.4",
    "nodemon": "^1.14.7",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.9",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.2.0",
    "react-dev-utils": "^4.2.1",
    "react-dom": "^16.2.0",
    "style-loader": "0.19.1",
    "styled-components": "^2.4.0",
    "svg-fill-loader": "^0.0.8",
    "svg-sprite-loader": "^3.6.2",
    "svgo": "^1.0.3",
    "svgo-loader": "^2.1.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.9.7",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "node client/scripts/start.js",
    "build": "node client/scripts/build.js",
    "test": "node client/scripts/test.js --env=jsdom",
    "server": "nodemon server/server.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn start\"",
    "deploy": "yarn build && gh-pages -d client/build"
  },
  "proxy": "http://localhost:5000/",
  "jest": {
    "collectCoverageFrom": [
      "client/src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/client/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/client/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/client/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/client/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/client/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "concurrently": "^3.5.1"
  }
}
